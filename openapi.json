{
  "openapi": "3.1.0",
  "info": {
    "title": "Deepcuts API",
    "description": "An API to access music artist metadata and recommendations.",
    "version": "v0.0.0"
  },
  "paths": {
    "/albums/{artist_name}": {
      "get": {
        "tags": [
          "deepcuts"
        ],
        "summary": "Get Albums By Artist",
        "description": "Get albums by artist name.",
        "operationId": "deepcuts-get_albums_by_artist",
        "parameters": [
          {
            "name": "artist_name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Artist Name"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetArtistAlbumsResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/recommend": {
      "post": {
        "tags": [
          "deepcuts"
        ],
        "summary": "Recommend Albums From Albums",
        "description": "Recommend albums based on shared artists.",
        "operationId": "deepcuts-recommend_albums_from_albums",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Body_deepcuts-recommend_albums_from_albums"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetRecommendationResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "404": {
            "description": "Not found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
