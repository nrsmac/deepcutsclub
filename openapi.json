            "items": {
              "type": "integer"
            },
            "type": "array",
            "title": "Credit Artist Ids",
            "default": []
          }
        },
        "type": "object",
        "required": [
          "title"
        ],
        "title": "Album",
        "description": "Schema for an album."
      },
